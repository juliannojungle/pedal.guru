{
    "configurations": [
        {
            "name": "rp2040",
            "includePath": [
                "${workspaceFolder}/**", //Including every directory since this doesn't appear to work.
                "${workspaceFolder}/src/API/**",
                "${workspaceFolder}/src/Dependency/pico-sdk/src",
                "${workspaceFolder}/src/Device/**",
                "${workspaceFolder}/src/GUI/**",
                "${workspaceFolder}/src/GUI/Interface/**",
                "${workspaceFolder}/src/GUI/Page/**",
                "${workspaceFolder}/src/GUI/Render/**",
                "${workspaceFolder}/src/Model/**",
                "${workspaceFolder}/src/Sensor/**",
                "${workspaceFolder}/src/Target/**",
                "${workspaceFolder}/src/Target/RP2040/**",
                "${workspaceFolder}/src/Target/Simulator/**",
                "${workspaceFolder}/src/Target"
            ],
            "defines": [
                "_DEBUG",
                "UNICODE",
                "_UNICODE",
                "TARGET_RP2040"
            ],
            "compilerPath": "/usr/bin/arm-none-eabi-g++",
            "cppStandard": "c++14",
            "intelliSenseMode": "linux-gcc-arm",
            "cStandard": "c11",
            "configurationProvider": "ms-vscode.cmake-tools"
        }
    ],
    "version": 4
}